<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:wpf="clr-namespace:nGratis.Cop.Olympus.Wpf">
  <wpf:AnyToLowerCaseConverter x:Key="Cop.Converter.AnyToLowerCaseConverter" />
  <wpf:AnyToTitleCaseConverter x:Key="Cop.Converter.AnyToTitleCaseConverter" />
  <wpf:AnyToTypeNameConverter x:Key="Cop.Converter.AnyToTypeNameConverter" />
  <wpf:AnyToUpperCaseConverter x:Key="Cop.Converter.AnyToUpperCaseConverter" />
  <BooleanToVisibilityConverter x:Key="Cop.Converter.BooleanToVisibilityConverter" />
  <wpf:BooleanToLogicalNotConverter x:Key="Cop.Converter.BooleanToLogicalNotConverter" />
  <wpf:ChartConfigToPlotModelConverter
    x:Key="Cop.Converter.ChartConfigToPlotModelConverter"
    ThemeManager="{DynamicResource Cop.ThemeManager}" />
  <wpf:ColorToSolidBrushConverter x:Key="Cop.Converter.ColorToSolidBrushConverter" />
  <wpf:EnumerableToCountConverter x:Key="Cop.Converter.EnumerableToCountConverter" />
  <wpf:EnumToValuesConverter x:Key="Cop.Converter.EnumToValuesConverter" />
</ResourceDictionary>