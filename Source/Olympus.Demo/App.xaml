<Application
  x:Class="nGratis.Cop.Olympus.Demo.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:olympus="http://schemas.ngratis.com/2014/olympus"
  xmlns:self="clr-namespace:nGratis.Cop.Olympus.Demo">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary>
          <self:AppBootstrapper x:Key="Bootstrapper" />
        </ResourceDictionary>
        <olympus:SharedResourceDictionary Source="/nGratis.Cop.Olympus.Wpf.Theme;component/Cop.Theme.Dark.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>